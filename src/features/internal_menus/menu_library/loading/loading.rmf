<stm>
<cursor 0>
<defaults noborder>
<include tints>
<tint lblue 0xffff8080>
<tint gray 0xff808080>
<tint black 0xff000000>
<tint vbargray 0xff606060>
<backdrop bgcolor black>

<frame header 640 95 page loading_header border 1 1 lblue backfill black>
<frame status 640 105 page loading_status border 1 1 gray backfill black>
<frame files 640 150 page loading_files border 1 1 gray backfill black>
<frame recent 640 130 page loading_recent border 1 1 gray backfill black>
</stm>
